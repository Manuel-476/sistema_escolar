        <servlet-class>Servlets.imprensao</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Alternativo</servlet-name>
        <url-pattern>/Alternativo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>excluir_fun</servlet-name>
        <url-pattern>/excluir_fun</url-pattern>
