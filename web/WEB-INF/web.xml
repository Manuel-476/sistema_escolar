    <servlet-mapping>
        <servlet-name>imprensao</servlet-name>
        <url-pattern>/imprensao</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
